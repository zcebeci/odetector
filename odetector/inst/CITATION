year <- sub("-.*", "", meta$Date) 
note <- sprintf("R package version %s", meta$Version)
textVersion = paste0("Cebeci, Z. & Yildiz, F. (", year,"). odetector: Outlier Detection Using Cluster Analysis. ", note, ", URL https://CRAN.R-project.org/package=odetector")
bibentry(textVersion, bibtype = "Manual",
 title = "{odetector}: Outlier Detection Using Cluster Analysis",
 author = c(person("Zeynel", "Cebeci"),
 person("Figen", "Yildiz")),
 year = year,
 note = note, 
 url = "https://CRAN.R-project.org/package=odetector")